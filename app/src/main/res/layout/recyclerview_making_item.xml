<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="200dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0">


        <ImageView
            android:id="@+id/making_arrow"
            android:layout_width="45dp"
            android:layout_height="45dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toStartOf="@+id/making_poi_idx_dst"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/baseline_arrow_right_alt_black_48dp" />

        <TextView
            android:id="@+id/making_poi_name_st"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:maxLines="2"
            android:minLines="2"
            android:singleLine="false"
            android:textAlignment="viewStart"
            android:textColor="@color/seoulRed"
            android:textSize="16dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_poi_idx_st"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.00999999"
            tools:text="Novotel" />

        <TextView
            android:id="@+id/making_poi_idx_st"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="5dp"
            android:layout_marginTop="5dp"
            android:textColor="@android:color/black"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:text="1" />

        <TextView
            android:id="@+id/making_subway_line_st"
            android:layout_width="140dp"
            android:layout_height="20dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="48dp"
            android:layout_marginEnd="216dp"
            android:gravity="center"
            android:textAlignment="viewStart"
            android:textColor="@color/seoulRed"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_subway_line_image_st"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.1"
            tools:text="Gyeongui-Jungang Line" />

        <ImageView
            android:id="@+id/making_subway_line_image_st"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.031"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/making_poi_name_st"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/dxline_icon" />

        <TextView
            android:id="@+id/making_subway_name_st"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:maxLines="3"
            android:minLines="3"
            android:text="Bongeunsa"
            android:textAlignment="viewStart"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_subway_line_image_st"
            app:layout_constraintTop_toBottomOf="@+id/making_subway_line_st"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/making_subway_name_dst"
            android:layout_width="140dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="3dp"
            android:maxLines="3"
            android:minLines="3"
            android:text="Bongeunsa"
            android:textAlignment="viewStart"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_subway_line_image_dst"
            app:layout_constraintTop_toBottomOf="@+id/making_subway_line_dst"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/making_subway_line_dst"
            android:layout_width="140dp"
            android:layout_height="20dp"
            android:layout_marginStart="3dp"
            android:layout_marginTop="48dp"
            android:gravity="center"
            android:textAlignment="viewStart"
            android:textColor="@color/seoulRed"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_subway_line_image_dst"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.1"
            tools:text="Gyeongui-Jungang Line" />

        <ImageView
            android:id="@+id/making_subway_line_image_dst"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_marginTop="16dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.6"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/making_poi_name_dst"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/dxline_icon" />

        <TextView
            android:id="@+id/making_poi_name_dst"
            android:layout_width="160dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:maxLines="2"
            android:minLines="2"
            android:singleLine="false"
            android:textAlignment="viewStart"
            android:textColor="@color/seoulRed"
            android:textSize="16dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/making_poi_idx_dst"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.005"
            tools:text="Novotel\n" />

        <TextView
            android:id="@+id/making_poi_idx_dst"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:textColor="@android:color/black"
            android:textSize="20dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.56"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:text="1" />

        <Button
            android:id="@+id/making_detail_button"
            android:layout_width="200dp"
            android:layout_height="50dp"
            android:background="@color/seoulRed"
            android:text=" Detailed subway route"
            android:textColor="@android:color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.967" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>